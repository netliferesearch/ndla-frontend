{
  "name": "ndla-frontend",
  "version": "0.1.16",
  "description": "NDLA Frontend",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "cross-env NODE_ENV=unittest jest",
    "tdd": "cross-env NODE_ENV=unittest jest --watch",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=development webpack --config ./webpack.config.prod.js -p --bail",
    "start": "cross-env NODE_ENV=development DISABLE_SSR=true nodemon server --watch server",
    "start-with-ssr": "cross-env NODE_ENV=development nodemon server --watch server",
    "start-prod": "cross-env NODE_ENV=development node server",
    "lint": "eslint --ext .js --ext .jsx src server"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test.(js|jsx)$",
    "setupFiles": [
      "./src/__tests__/_initTestEnv.js"
    ]
  },
  "engines": {
    "node": "~6.6.0",
    "npm": "~3.10.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
